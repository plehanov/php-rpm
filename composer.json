{
    "name": "plehanov/php-rpm",
    "description": "RPM packager for PHP applications.",
    "license": "GPL-3.0",
    "keywords": ["rpm", "packager", "package building"],
    "version": "1.0.2",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Plehanov\\RPM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-phar": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.5"
    }
}
